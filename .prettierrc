{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": "docs/tablas/*.md",
      "options": {
        "parser": "markdown",
        "proseWrap": "preserve",
        "printWidth": 200
      }
    },
    {
      "files": "*.md",
      "options": {
        "parser": "markdown",
        "proseWrap": "preserve",
        "printWidth": 150
      }
    }
  ]
}
